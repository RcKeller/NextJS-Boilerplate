    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "less": "^2.7.3",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^1.0.6",
    "method-override": "^2.3.10",
    "morgan": "^1.9.1",
    "next": "^7.0.0",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "nprogress": "^0.2.0",
    "openssl-self-signed-certificate": "^1.1.6",
